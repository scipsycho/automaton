{
  "manifest_version": 2,
  "name": "Automaton-test",
  "description": "Automate everything!",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "logo69x69.jpg"
  },
  "background": {
    "scripts": ["./scripts/background/main.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./scripts/content/main.js"]
    }
  ]
}